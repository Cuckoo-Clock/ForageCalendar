    "npm": "^6.14.9",
    "onchange": "^7.0.0",
    "parallelshell": "3.0.1"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}

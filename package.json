{
  "name": "foragerOne",
  "version": "1.0.0",
  "description": "a website to assist in foraging mushrooms in RI.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
